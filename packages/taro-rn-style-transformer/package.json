{
  "name": "@tarojs/rn-style-transformer",
  "version": "4.0.0-beta.66",
  "description": "提供 Taro RN 统一处理样式文件能力",
  "author": "O2Team",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "pnpm run clean",
    "clean": "rimraf --impl=move-remove ./dist",
    "build": "tsc",
    "dev": "tsc -w",
    "lint": "eslint src --ext .js,.ts",
    "test": "cross-env NODE_OPTIONS=\"--experimental-vm-modules --no-warnings --loader ts-node/esm\" jest",
    "test:ci": "jest --ci -i --coverage --silent",
    "test:clear": "jest --clearCache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NervJS/taro.git"
  },
  "bugs": {
    "url": "https://github.com/NervJS/taro/issues"
  },
  "engines": {
    "node": ">= 18"
  },
  "dependencies": {
    "@react-native/metro-babel-transformer": "^0.73.2",
    "@tarojs/helper": "workspace:*",
    "fbjs": "^3.0.5",
    "less": "^4.2.0",
    "postcss-import": "^16.1.0",
    "postcss-pxtransform": "workspace:*",
    "postcss-reporter": "^7.1.0",
    "prop-types": "^15.8.1",
    "resolve": "^1.22.8",
    "sass": "^1.75.0",
    "stylelint": "^16.4.0",
    "stylelint-config-taro-rn": "workspace:*",
    "stylelint-taro-rn": "workspace:*",
    "stylus": "^0.63.0",
    "taro-css-to-react-native": "workspace:*"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "postcss": "^8.4.38",
    "react": "^18.2.0"
  },
  "peerDependencies": {
    "postcss": ">= 8"
  }
}
