{
  "name": "stylelint-taro-rn",
  "description": "A collection of React Native specific rules for stylelint",
  "author": "O2Team",
  "license": "MIT",
  "version": "4.0.0-beta.66",
  "main": "dist/index.js",
  "files": [
    "dist",
    "src",
    "CHANGELOG.md",
    "README.md",
    "!**/__tests__"
  ],
  "keywords": [
    "css",
    "csslint",
    "lint",
    "linter",
    "stylelint",
    "stylelint-plugin"
  ],
  "scripts": {
    "prebuild": "pnpm run clean",
    "clean": "rimraf --impl=move-remove ./dist",
    "build": "babel src --out-dir ./dist",
    "test": "cross-env NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NervJS/taro.git"
  },
  "engines": {
    "node": ">= 18"
  },
  "dependencies": {
    "react-native-known-styling-properties": "^1.3.0"
  },
  "devDependencies": {
    "jest-light-runner": "^0.6.0",
    "jest-preset-stylelint": "^7.0.0",
    "lodash": "^4.17.21"
  },
  "peerDependencies": {
    "stylelint": "^16"
  }
}
