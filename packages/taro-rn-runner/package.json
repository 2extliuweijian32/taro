{
  "name": "@tarojs/rn-runner",
  "version": "4.0.0-beta.66",
  "description": "ReactNative build tool for taro",
  "author": "O2Team",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "prebuild": "pnpm run clean",
    "clean": "rimraf --impl=move-remove ./dist",
    "build": "tsc",
    "dev": "tsc -w",
    "lint": "eslint src templates --ext .js,.ts",
    "test": "jest --silent",
    "test:ci": "jest --ci -i --coverage --silent",
    "test:coverage": "jest --coverage",
    "test:dev": "jest --watch",
    "updateSnapshot": "jest --updateSnapshot"
  },
  "keywords": [
    "taro"
  ],
  "bugs": {
    "url": "https://github.com/NervJS/taro/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NervJS/taro.git"
  },
  "engines": {
    "node": ">= 18"
  },
  "dependencies": {
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@tarojs/helper": "workspace:*",
    "@tarojs/rn-style-transformer": "workspace:*",
    "@tarojs/rn-supporter": "workspace:*",
    "@tarojs/rn-transformer": "workspace:*",
    "acorn": "^8.11.3",
    "acorn-jsx": "^5.3.2",
    "fs-extra": "^11.2.0",
    "lodash": "^4.17.21",
    "rollup-plugin-image-file": "^1.0.2"
  },
  "devDependencies": {
    "@rnx-kit/metro-resolver-symlinks": "^0.1.36",
    "babel-plugin-transform-react-jsx-to-rn-stylesheet": "workspace:*",
    "expo": "~51.0.5",
    "expo-file-system": "~16.0.0",
    "react": "^18.2.0",
    "react-native": "^0.72.3",
    "rollup": "^4.16.4"
  },
  "peerDependencies": {
    "expo": ">= 51",
    "react": ">= 18",
    "react-native": ">= 0.63.0"
  }
}
